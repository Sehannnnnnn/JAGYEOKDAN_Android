<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/white">

    <LinearLayout
        android:id="@+id/header_line"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:text="자격증"
            android:fontFamily="@font/a_title_gothic3"
            android:textSize="20sp"
            android:textColor="@color/black"
            android:gravity="center" />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-10dp"
            android:layout_marginBottom="24dp"
            android:text="Category"
            android:fontFamily="@font/golden_plains_demo"
            android:textSize="25sp"
            android:textColor="@color/black"
            android:gravity="center" />
        <View
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:background="#C4C4C4"
            android:layout_marginHorizontal="40dp"/>

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/header_line"
        android:layout_marginTop="30dp"
        android:layout_marginHorizontal="40dp">

        <LinearLayout
            android:id="@+id/license_first_col"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/license_button_1"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/license_circle"
                    android:gravity="center"
                    android:layout_gravity="center">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/license_icon1"/>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:text="경영·사무"
                    android:textColor="#979797"
                    android:layout_gravity="center"
                    android:fontFamily="@font/a_title_gothic2"
                    android:textSize="10sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/license_button_2"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/license_circle"
                    android:gravity="center"
                    android:layout_gravity="center">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon2"/>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:text="마케팅·광고·홍보"
                    android:textColor="#979797"
                    android:fontFamily="@font/a_title_gothic2"
                    android:layout_gravity="center"
                    android:textSize="10sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/license_button_3"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/license_circle"
                    android:layout_gravity="center"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon3"/>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_marginTop="7dp"
                    android:layout_height="wrap_content"
                    android:text="IT·인터넷"
                    android:textColor="#979797"
                    android:layout_gravity="center"
                    android:fontFamily="@font/a_title_gothic2"
                    android:textSize="10sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/license_button_4"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/license_circle"
                    android:gravity="center"
                    android:layout_gravity="center">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon4"/>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:text="디자인"
                    android:textColor="#979797"
                    android:fontFamily="@font/a_title_gothic2"
                    android:layout_gravity="center"
                    android:textSize="10sp"/>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/license_second_col"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="30dp">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/license_button_5"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/license_circle"
                    android:gravity="center"
                    android:layout_gravity="center">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon5"/>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:text="무역·유통"
                    android:textColor="#979797"
                    android:layout_gravity="center"
                    android:fontFamily="@font/a_title_gothic2"
                    android:textSize="10sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/license_button_6"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/license_circle"
                    android:gravity="center"
                    android:layout_gravity="center">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon6"/>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:text="영업·고객상담"
                    android:textColor="#979797"
                    android:fontFamily="@font/a_title_gothic2"
                    android:layout_gravity="center"
                    android:textSize="10sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/license_button_7"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/license_circle"
                    android:layout_gravity="center"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon7"/>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_marginTop="7dp"
                    android:layout_height="wrap_content"
                    android:text="서비스"
                    android:textColor="#979797"
                    android:layout_gravity="center"
                    android:fontFamily="@font/a_title_gothic2"
                    android:textSize="10sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/license_button_8"
                    android:layout_width="50dp"
