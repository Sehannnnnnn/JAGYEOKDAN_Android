                    android:textColor="#979797"
                    android:layout_gravity="center"
                    android:fontFamily="@font/a_title_gothic2"
                    android:textSize="10sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/license_button_10"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/license_circle"
                    android:gravity="center"
                    android:layout_gravity="center">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon10"/>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:text="교육"
                    android:textColor="#979797"
                    android:fontFamily="@font/a_title_gothic2"
                    android:layout_gravity="center"
                    android:textSize="10sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/license_button_11"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/license_circle"
                    android:layout_gravity="center"
                    android:gravity="center">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon11"/>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_marginTop="7dp"
                    android:layout_height="wrap_content"
                    android:text="건설"
                    android:textColor="#979797"
                    android:layout_gravity="center"
                    android:fontFamily="@font/a_title_gothic2"
                    android:textSize="10sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_weight="1">
                <LinearLayout
                    android:id="@+id/license_button_12"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/license_circle"
                    android:gravity="center"
                    android:layout_gravity="center">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon12"/>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:text="의료"
                    android:textColor="#979797"
                    android:fontFamily="@font/a_title_gothic2"
                    android:layout_gravity="center"
                    android:textSize="10sp"/>
            </LinearLayout>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/license_forth_col"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="30dp">
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="15dp">
                <LinearLayout
                    android:id="@+id/license_button_13"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/license_circle"
                    android:gravity="center"
                    android:layout_gravity="left">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon13"/>
                </LinearLayout>
                <TextView
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:text="미디어"
                    android:textColor="#979797"
                    android:layout_gravity="center"
                    android:fontFamily="@font/a_title_gothic2"
                    android:textSize="10sp"/>
            </LinearLayout>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginStart="35dp">
                <LinearLayout
                    android:id="@+id/license_button_14"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/license_circle"
                    android:gravity="center"
                    android:layout_gravity="left">
                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon14"/>
                </LinearLayout>
                <TextView
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:text="전문·특수직"
                    android:textColor="#979797"
                    android:fontFamily="@font/a_title_gothic2"
                    android:layout_gravity="left"
                    android:textSize="10sp"/>
            </LinearLayout>
        </LinearLayout>



    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>