<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <!--검색창-->
    <LinearLayout
        android:id="@+id/top_container"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="@drawable/container_design"
        android:orientation="horizontal"
        android:paddingVertical="10dp"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/top_Small_logo"
            android:layout_width="60dp"
            android:layout_height="30dp"
            android:layout_marginStart="15dp"
            android:src="@drawable/small_logo" />

        <RelativeLayout
            android:id="@id/search_bar"
            android:layout_width="250dp"
            android:layout_height="match_parent"
            android:layout_marginStart="15dp"
            android:background="@drawable/search_bar">

            <EditText
                android:id="@+id/search_keyword"
                android:layout_width="240dp"
                android:layout_height="match_parent"
                android:layout_marginStart="10dp"
                android:background="@drawable/search_bar"
                android:importantForAutofill="no"
                android:inputType="text" />

            <Button
                android:id="@+id/top_search_button_license"
                android:layout_width="15dp"
                android:layout_height="15dp"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="10dp"
                android:background="@drawable/search_icon" />

        </RelativeLayout>

        <View
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_weight="1" />

        <Button
            android:id="@+id/watch_alarm_button_license"
            android:layout_width="19dp"
            android:layout_height="22dp"
            android:layout_marginHorizontal="15dp"
            android:background="@drawable/alarm_icon" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/header_line"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/top_container">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="24dp"
            android:fontFamily="@font/a_title_gothic3"
            android:gravity="center"
            android:text="자격증"
            android:textColor="@color/black"
            android:textSize="20sp"
            tools:ignore="HardcodedText" />

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="-10dp"
            android:layout_marginBottom="24dp"
            android:fontFamily="@font/golden_plains_demo"
            android:gravity="center"
            android:text="Category"
            android:textColor="@color/black"
            android:textSize="25sp"
            tools:ignore="HardcodedText" />

        <View
            android:layout_width="wrap_content"
            android:layout_height="1dp"
            android:layout_marginHorizontal="40dp"
            android:background="#C4C4C4" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="40dp"
        android:layout_marginTop="30dp"
        android:orientation="vertical"
        app:layout_constraintTop_toBottomOf="@+id/header_line">

        <LinearLayout
            android:id="@+id/license_first_col"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:baselineAligned="false"
            android:orientation="horizontal">

            <!--경영/사무-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_1"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/license_icon1" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:text="경영·사무"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

            <!--마케팅/광고/홍보-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_2"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon2" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:text="마케팅·광고·홍보"
                    android:textAlignment="center"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

            <!--IT/인터넷-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_3"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon3" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:text="IT·인터넷"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

            <!--디자인-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_4"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon4" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:text="디자인"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/license_second_col"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:baselineAligned="false"
            android:orientation="horizontal">

            <!--무역/유통-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_5"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon5" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:text="무역·유통"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

            <!--영업/고객상담-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_6"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon6" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:text="영업·고객상담"
                    android:textAlignment="center"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

            <!--서비스-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_7"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon7" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:text="서비스"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

            <!--연구개발/설계-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_8"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon8" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:text="연구개발·설계"
                    android:textAlignment="center"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/license_third_col"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:baselineAligned="false"
            android:orientation="horizontal">

            <!--생산/제조-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_9"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon9" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:text="생산·제조"
                    android:textAlignment="center"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

            <!--교육-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_10"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon10" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:text="교육"
                    android:textAlignment="center"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

            <!--건설-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_11"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon11" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:text="건설"
                    android:textAlignment="center"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

            <!--의료-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_12"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_gravity="center"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon12" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:text="의료"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/license_forth_col"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:orientation="horizontal">

            <!--미디어-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_13"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon13" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:gravity="center"
                    android:text="미디어"
                    android:textAlignment="center"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

            <!--전문/특수직-->
            <LinearLayout
                android:layout_width="55dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:gravity="center"
                android:orientation="vertical">

                <LinearLayout
                    android:id="@+id/license_button_14"
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:background="@drawable/selector_license_icon"
                    android:gravity="center">

                    <ImageView
                        android:layout_width="25dp"
                        android:layout_height="25dp"
                        android:src="@drawable/lisence_icon14" />

                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="7dp"
                    android:fontFamily="@font/a_title_gothic2"
                    android:gravity="center"
                    android:text="전문·특수직"
                    android:textColor="#979797"
                    android:textSize="10sp"
                    tools:ignore="HardcodedText, SmallSp" />

            </LinearLayout>

            <View
                android:layout_width="55dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

            <View
                android:layout_width="55dp"
                android:layout_height="0dp"
                android:layout_weight="1" />

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>